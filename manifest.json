{
  "manifest_version": 3,
  "name": "Goto Processed URL",
  "version": "0.4.0",
  "description": "On click, rewrite current tab URL and navigate in-place.",
  "permissions": ["tabs", "storage"],
  "action": { "default_title": "Process URL and go" },
  "background": { "service_worker": "background.js", "type": "module" },
  "options_page": "options.html"
}
